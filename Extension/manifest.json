{
    "name": "Assignment 3",
    "description": "List extension",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "16": "icon_top.png"
    },
    "permissions": [],
    "web_accessible_resources": [
        "/logic/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}